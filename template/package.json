{
  "name": "simple-template",
  "version": "1.4.0",
  "description": "A simple webpack-config for multiple entries",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --config ./webpack.config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --progress --config ./webpack.config/webpack.prod.js",
    "start": "npm run dev"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "html-withimg-loader": "^0.1.16",
    "less-loader": "^4.0.5",
    "portfinder": "^1.0.13",
    "postcss-loader": "^2.0.8",
    "style-loader": "^0.19.0",
    "stylus-loader": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "Firefox ESR",
    "ie >= 8",
    "iOS >= 8",
    "Android > 4.4",
    "not Opera <= 12.1"
  ],
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true,
          "moduleName": "babel-runtime"
        }
      ],
      "syntax-dynamic-import"
    ]
  }
}
